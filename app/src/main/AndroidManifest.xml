<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.complaintsapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ComplaintsApp">
        <activity android:name=".CmplaintStatusScreen"></activity>
        <activity android:name=".Complaint_list" />
        <activity
            android:name=".AdminActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".SignupActivity"
            android:exported="false"
            android:label="Sign Up!"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".CompletedActivity"
            android:exported="false"
            android:label="Its Done!"
            android:parentActivityName=".ComplaintActivity" />
        <activity
            android:name=".ComplaintActivity"
            android:exported="false"
            android:label="Complaint"
            android:parentActivityName=".LoginActivity" />
        <activity
            android:name=".LoginActivity"
            android:exported="false"
            android:label="Welcome!!"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>